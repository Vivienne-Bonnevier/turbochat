<%= form_with model: @room, remote: true, class: "", data: { controller:"reset-form", action: "turbo:submit-end->reset-form#reset" } do |f| %>
  <%= f.text_field :name, class: "w-1/2" , autocomplete: "off" %>
  <%= f.submit data:{disable_with: false}, class: "text-black bg-transparent hover:bg-fuchsia-500 text-fuchsia-700 font-semibold hover:text-white py-2 px-4 border border-fuchsia-500 hover:border-transparent rounded" %>
<% end %>